	Subroutine BZ_Coords_EL1(Qc,nR,nC,xLx,xUx,xLy,iGeo,iOut)
!	xLx=16/xUx=32	
	Implicit Real(kind=8) (a-h,o-z)
	real(kind = 8)	Qc
	Dimension		Qc(nR,nC)
	real(kind = 8)	xLx,xUx,xLy,xLy3,x2Ly3,aa
	integer			nR,nC
!
	Qc			= 0.			! zero all
!
!------------------------------ 
	if(iGeo == 0) Then			!---------------------- C0
		Qc(1,1)		= 0.0
		Qc(1,2)		= xLx/3.
		Qc(1,3)		= 2.*xLx/3.
		Qc(1,4)		= xLx
!	-----------------------------
		aa			= xLx + (xUx - xLx)/3.
!	-----------------------------
		Qc(1,5)		= 0.0
		Qc(1,6)		= aa/3.
		Qc(1,7)		= 2.*aa/3.
		Qc(1,8)		= aa
!	-----------------------------
		aa			= xLx + 2.*(xUx - xLx)/3.
!	-----------------------------
		Qc(1,9)		= 0.0
		Qc(1,10)	= aa/3.
		Qc(1,11)	= 2.*aa/3.
		Qc(1,12)	= aa
		Qc(1,13)	= 0.0
		Qc(1,14)	= xUx/3.
		Qc(1,15)	= 2.*xUx/3.
		Qc(1,16)	= xUx
!------------------------------ 
	else if(iGeo == 2) Then		!---------------------- C2
		Qc(1,2)		= 4.0
		Qc(1,3)		= 8.0
		Qc(1,4)		= xLx
!	-----------------------------
		aa			= xLx + (xUx - xLx)/3.
!	-----------------------------
		Qc(1,6)		= 6.0
		Qc(1,7)		= aa - 8.0
		Qc(1,8)		= aa
!	-----------------------------
		aa			= xLx + 2.*(xUx - xLx)/3.
!	-----------------------------
		Qc(1,10)	= 8.0
		Qc(1,11)	= aa - 8.0
		Qc(1,12)	= aa
		Qc(1,14)	= 12.0
		Qc(1,15)	= xUx - 8.0
		Qc(1,16)	= xUx
!------------------------------ 
	else if(iGeo == 1) Then		!---------------------- C1
!----------------------- C1
		Qc(1,2)		= 8.0D0/3.0D0
		Qc(1,3)		= 8.0
		Qc(1,4)		= xLx
!	-----------------------------
		aa			= xLx + (xUx - xLx)/3.
!	-----------------------------
		Qc(1,6)		= 56.0D0/9.0D0
		Qc(1,7)		= aa - 8.0
		Qc(1,8)		= aa
!	-----------------------------
		aa			= xLx + 2.*(xUx - xLx)/3.
!	-----------------------------
		Qc(1,10)	= 88.0D0/9.0D0
		Qc(1,11)	= aa - 8.0
		Qc(1,12)	= aa
		Qc(1,14)	= 40.0D0/3.0D0
		Qc(1,15)	= xUx - 8.0
		Qc(1,16)	= xUx
	end if
!----------------------- Zero Twist Override
!	Qc(1,6)		= 16.0D0/3.0D0
!	Qc(1,7)		= 16.0D0
!	Qc(1,10)	= 32.0D0/3.0D0
!	Qc(1,11)	= 16.0D0
!-----------------------------
!
	xLy3		= xLy/3.
	x2Ly3		= 2.*xLy3
!
!	Qc(2,1)		= 0.
!	Qc(2,2)		= 0.
!	Qc(2,3)		= 0.
!	Qc(2,4)		= 0
!
	Qc(2,5)		= xLy3
	Qc(2,6)		= xLy3
	Qc(2,7)		= xLy3
	Qc(2,8)		= xLy3

!
	Qc(2,9)		= x2Ly3
	Qc(2,10)	= x2Ly3
	Qc(2,11)	= x2Ly3
	Qc(2,12)	= x2Ly3

!
	Qc(2,13)	= xLy
	Qc(2,14)	= xLy
	Qc(2,15)	= xLy
	Qc(2,16)	= xLy
  100 continue
!
	return
	end
	
