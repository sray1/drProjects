void CMs2CreOM::Init_PAT()
{
	Init_NOD();
	//////////////////
	CDrGrafDoc* pDoc 		= ((CDrGrafApp*)AfxGetApp())->GetDocument(); 
	CObjectMgr* pObjectMgr 	= pDoc->GetObjectMgr();
	CMsObjMgr* pMsObjMgr 	= pDoc->GetMsObjMgr();
	/////////////////////////////
	CMI_Gang0 Gang0;
	Gang0.SetElemInfoID(m_SpawnedID,MESH2D);		
	/////////////////////////////////////////// Node
	m_pSpawnedMesh2D		= NULL;
	m_pParentMesh2D			= NULL;
	m_ParentID				= "";
	m_pCurrentMesh2D		= NULL;
	/////////////////////////////////////////// Dlg_CrOI 
	m_NID			= m_ParentID;
	/////////////////////////////////////////// STitch
	m_p2Profile		= NULL;
	m_pLastMesh2D	= NULL;
	m_Mesh2DList.RemoveAll();

}

void CMs2CreOM::Terminate_PAT(BOOL bError)
{
	Terminate_NOD(bError);
	////
	if(bError)
	{
		/////////////////////////////////////////// 
		if(m_pSpawnedMesh2D)
		{
			delete m_pSpawnedMesh2D;
			m_pSpawnedMesh2D = NULL;
		}
		/////////////////////////////////////////// 
		if(m_p2Profile)
		{
			delete m_p2Profile;
			m_p2Profile		= NULL;
			m_pCurrentMesh2D	= NULL;
			m_pLastMesh2D	= NULL;
			m_Mesh2DList.RemoveAll();
		}
		///////////////////////////
	}
}

