			if(ShowMesh2DToSpawn(m_pParentMesh2D,FALSE)<0)		 //FALSE = NoDlg
			////////////////
			{
				Terminate_PAT(TRUE);
				return;
			}
			else
			{
				Terminate_PAT(FALSE);
				Init_PAT();
				/////////////////////////
				UpdateDlg_FALSE_SHEAR();
				break;
			}
			///////////////////	
			break;
		

		
		case COP_SPAWN:
		
			//////////////////////////// 	
			return;



		case COP_STITCH:
		
			/////////////////////////////
			if(Show2ProfileToStitch()<0)		
			////////////////
			{
				Terminate_PAT(TRUE);
				return;
			}
			else
