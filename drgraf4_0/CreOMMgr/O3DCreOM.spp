			////////////////
			{
				Terminate_O3D(TRUE);
				return;
			}
			else
			{
				Terminate_O3D(FALSE);
				Init_O3D();
				/////////////////////////
				UpdateDlg_FALSE_REFLECT();
				break;
			}
			///////////////////	
			break;
		
		case COP_SHEAR:
						
			
			SetUpLocalM_SHEAR();
			/////////////////////////////
			if(ShowMbj3DToSpawn(m_pParentMbj3D,FALSE)<0)		 //FALSE = NoDlg
			////////////////
			{
				Terminate_O3D(TRUE);
				return;
			}
			else
			{
				Terminate_O3D(FALSE);
				Init_O3D();
				/////////////////////////
				UpdateDlg_FALSE_SHEAR();
				break;
			}
			///////////////////	
