}

void CO3DCreOM::Terminate_O3D(BOOL bError)
{
	Terminate_NOD(bError);
	////
	if(bError)
	{
		/////////////////////////////////////////// 
		if(m_pSpawnedMbj3D)
		{
			delete m_pSpawnedMbj3D;
			m_pSpawnedMbj3D = NULL;
		}
		///////////////////////////
		if(m_pMProfile)
		{
			delete m_pMProfile;
			m_pMProfile		= NULL;
			m_pCurrentMbj3D	= NULL;
			m_pLastMbj3D	= NULL;
			m_Mbj3DList.RemoveAll();
		}
		///////////////////////////
	}
}

