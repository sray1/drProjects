int CO3DCreOM::ShowMbj3DToSpawn(CDrMbj3D* pParentMbj3D,BOOL bUsingDlg)
{

	CDrGrafFrame*	pWnd	= (CDrGrafFrame*)AfxGetMainWnd();
	CString			strCurr;
	////////////
	CDrGrafDoc* pDoc 		= ((CDrGrafApp*)AfxGetApp())->GetDocument(); 
	CObjectMgr* pObjectMgr  = pDoc->GetObjectMgr();
	CDListMgr* pDListMgr;
	////////////////////////////////
	strCurr = "Please Wait";
	///////////////////////// CurrStat: Show
	pWnd->GetCurrStat()->SetText(strCurr);
	pWnd->GetCurrStat()->Invalidate();
	pWnd->GetCurrStat()->UpdateWindow();
	///////////////////
	pObjectMgr->SetActiveObjectType(MBJ3D);
	////////////////////////////////////////////////////////////////////////////// Add to DataBase
	int nActiveIndex;
	///////////////
	m_pSpawnedMbj3D	= (CDrMbj3D*)(pObjectMgr->AddToDataBase(nActiveIndex,MBJ3D));
	m_pSpawnedMbj3D->SetShow(FALSE);	// not quite ready
	/////////////////////////////////////////////////////////////// Using CMI_CSPN
	CMI_OSPN MI_OSPN;
	int nResult;
	if(bUsingDlg)
	{
		m_pSpawnedMbj3D->SetObjectID(m_ObjID);
		///
		nResult = MI_OSPN.SpawnMbj3DShowingDlg(m_pSpawnedMbj3D,pParentMbj3D,pParentMbj3D,MBJ3D);
	}
	else
	{
		pMATRIX	pXFMat	= m_pMatObj->GetMatrix();
		CString	SpawnID	= m_ObjID;
		////
		nResult = MI_OSPN.SpawnMbj3DWithoutDlg(m_pSpawnedMbj3D,pParentMbj3D,pXFMat,&m_SpawnedID,
												pParentMbj3D,MBJ3D); 
	}
	if(nResult<0)
	{
		/////////////////////////////////////// Reset
		m_nInfoDone		= 0;
		m_bDone			= TRUE;	// 
		////////////////////////// Delete
		pObjectMgr->DeleteFromDataBase(nActiveIndex, MBJ3D);
		m_pSpawnedMbj3D	= NULL;
		//////////////////////////
		return nResult;//
	}
	/////////////////////////////////////////////////////////// Control RectBound
	m_pSpawnedMbj3D->SetDirty(TRUE);
	m_pSpawnedMbj3D->SetShow(TRUE);
	////////////////////////////////////////////////////////// Save
	CString ID		= m_pSpawnedMbj3D->GetObjectID();
	pDListMgr	 	= pObjectMgr->GetObjectList(MBJ3D);
	int nIndex		= pDListMgr->GetObjectIndex(MBJ3D,ID); 
	int nOldIndex 	= pObjectMgr->GetActiveObjectIndex();
	pObjectMgr->SetActiveObjectIndex(nIndex); 
	pObjectMgr->SetActiveObjectType(MBJ3D); 
    ////////////////////////////////////////////////////////// done/Invalidate
	pObjectMgr->UpdateAllViewsWithActiveIndex(NULL,nIndex,MBJ3D);
	///////////////////////////////
	return 0;
}	 

