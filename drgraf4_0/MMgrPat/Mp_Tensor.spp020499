int CMP_Tensor::CreateEndTanListForACurve(CDrCurve* pCurve,CList<VECTOR,VECTOR>& EndTanList)
{
	int nResult = MA_OK;
	//////////////////////////////////////////////////////////////////////
	//		4 tangents created	
	//		and are
	//			tangent at CNode_0
	//			tangent at CNode_1
	//			tangent at CNode_Max -1
	//			tangent at CNode_Max
	//////////////////////////////////////////////////////////////////////			
	CDrNode*	pCNode;
	WORLD		wNodeCoord;
	VECTOR		tan[4];
	POSITION	pos;
	int			i,nConBZ,index;
	////////////////
	imax = pCurveList->GetCount();
	if(imax<1)
		return MA_ERROR;
	////////////////////////////////////////////////////////////////////////// Curve0
	pos = pCurveList->FindIndex(0);
	CDrCurve* pCurve	= (CDrCurve*)pCurveList->GetAt(pos);
	nConBZ = pCurve->GetCNodeList()->GetCount();
	////////////////////////////////////////////// tan_0
	pCurve->Get_EndTangent(tan[0],TRUE/*bBegin*/);
	////////////////////////////////////////////// tan_1
	pos			= pCurve->GetCNodeList()->FindIndex(1); // 2nd Node
	pCNode		= (CDrNode*)(GetCNodeList()->GetAt(pos));
	wNodeCoord	= *(pCNode->GetLocalPos());
	pCurve->Get_TanVectorAtaNode(wNodeCoord,tan[1]);
	////////////////////////////////////////////// tan_Max - 1
	index		= pCurve->GetCNodeList()->GetCount() - 2; 
	pos			= pCurve->GetCNodeList()->FindIndex(index); // penulty Node
	pCNode		= (CDrNode*)(GetCNodeList()->GetAt(pos));
	wNodeCoord	= *(pCNode->GetLocalPos());
	pCurve->Get_TanVectorAtaNode(wNodeCoord,tan[2]);
	////////////////////////////////////////////// tan_Max
	pCurve->GetEndTangent(tan[3],FALSE/*bBegin*/);
	///////////////////////////////////////////////////////// save
	for(i=0;i<4;i++)
		EndTanList->AddTail(tan[i]);
	/////////////
	return MA_OK;
}
	//////////////////////////////////////////////////// CornerTwists
	EstimateCornerTwists(pNodeList,pTanList,n_Nodes_U,n_Nodes_V,pCornerTwist,TwistMethod);
	//////////////////////////////////////////////////// Extend BoundaryCond Using Twist
?????????????
}

int CMP_Tensor::EstimateCornerTwists(CDListMgr* pNodeList,TWIST TwistMethod,
									 CDListMgr* pTanList_U,CDListMgr* pTanList_V,
									 int n_Nodes_U,int n_Nodes_V,pWORLD CornerTwist[][2])
{
	int nResult = MA_OK;
	////////////////////////////////////////////////////
	CTwist	Twist;
	WORLD	w0{0.,0.,0.};
	////////////////////
	switch (TwistMethod)
	{
		case TW_AUTO:	// fall through
		case TW_ZERO:
			/////////////////////////// Copy zero Twist
			for(i=0;i<2;i++)
				for(j=0;i<2;i++)
					CornerTwist[i][j] = w0;
			break;
		case TW_BESSEL:
			Twist.Bessel(pNodeList,n_Nodes_U,n_Nodes_V,CornerTwist);
			break;
		case TW_ADINI:
			Twist.Adini(pNodeList,pTanList_U,pTanList_V,n_Nodes_U,n_Nodes_V,CornerTwist);
			break;
		case TW_SPLINE:
			Twist.Spline(pNodeList,pTanList_U,pTanList_V,n_Nodes_U,n_Nodes_V,CornerTwist);
			break;
		case TW_ENERGY:
			Twist.Energy(pNodeList,n_Nodes_U,n_Nodes_V,CornerTwist);
			break;
		case TW_USER:
			break;		// nothing to do
		default:
			return MA_ERROR;
	}
	/////////////
	return MA_OK;
}

	 
