//////////////////////////////////////////////////////////////////////////////////////////////
//		WE CHOSE TO TREAT UNIVARIATE IN U-DIR FIRST FOLLOWED BY
//					UNIVARIATE IN V-DIR. 
//		Thus,
//			EndList_V Needs to be Extended to to Create
//			univariate V-curves at NON-LATTICE Bezier
//			Interior Pts. of U-Curves.
//		This in turn may need
//			Estimation of Twist at Surface 4 Corners which
//			along with LATTICE EndInfo_V can estimate
//			NON-LATTICE V-Curves using same BlendingCurve_V						
//////////////////////////////////////////////////////////////////////////////////////////////
// TensorProduct Patches Could be arising out of 2 situations:
//		//////////////////////////																		
//		1. As Part of Gordon Surfaces
//			In this Case:
//				bGordonPart		=	TRUE;
//				pCurveList_U	=	Curves Along U(ie V=const.Curves)
//				pEndList_U		=	EndInfo of Curves Along U<--------\
//				pCurveList_V	=	Curves Along V(ie U=const.Curves)  |--- May Need for Twist
//				pEndList_V		=	EndInfo of Curves Along V<--------/		based on TwistMethod
//		//////////////////////////																		
//		2. As StandAlone w/ Lattice Nodes 
//			In this Case:
//				bGordonPart		=	FALSE;
//				pCurveList_U	=	EMPTY
//				pEndList_U		=	EndInfo of Lattice Pts Along U
//				pCurveList_V	=	EMPTY
//				pEndList_V		=	EndInfo of Lattice Pts Along V
//////////////////////////////////////////////////////////////////////////////////////////////
int CMP_Tensor::GoDoIt 
				(
					BOOL							bGordonPart,
					CString&						PatchID,		
					CDrPatch*						pDrPatch,
					CDListMgr*						pNodeList,
					//////////////////////////////////////////////
					pWORLD							pCornerTwist[][2],// it is full,if TW_USER
					TWIST							TwistMethod,
					//////////////////////////////////////////////
					int								n_Nodes_U,
					CList<int,int>&					ElperSegList_U,
					double							Ratio_U,
					CDListMgr*						pCurveList_U,
					CList<CURVELATCH,CURVELATCH>*	pLatchList_U,
					CDrCurve*						pBlendCurve_U,
					CList<BLENDEND,BLENDEND>*		pEndList_U,// if(bClosed_U) EndList Empty
					//////////////////////////////////////////////
					int								n_Nodes_V,
					CList<int,int>&					ElperSegList_V,
					double							Ratio_V,
					CDListMgr*						pCurveList_V,
					CList<CURVELATCH,CURVELATCH>*	pLatchList_V,
					CDrCurve*						pBlendCurve_V,
					CList<BLENDEND,BLENDEND>*		pEndList_V,// if(bClosed_V) EndList Empty
					//////////////////////////////////////////////
					CView*							pView
				) 
{
	int nResult = MA_OK;
	///////////////////////
	m_PatchID	= PatchID;
	m_pDrPatch	= pDrPatch;
	CList<BLENDEND,BLENDEND> pEndListExt_V;
	/////////////////////// Stand Alone & NOT Part of Gordon
	if(bGordonPart)
	{
		/////////////////////////////////////////////////////////////////////////// Fill All Info_S
		/////////////////////////////////////////////////////////////////////////// Fill All Info_T
	}
	else	// create Lattice Curves & EndInfo
	{
		/////////////////////////////////////////////////////////////////////////// U,S-direction
		nResult = LatticeCurves_U(PatchID,pNodeList,n_Nodes_V,n_Nodes_U,ElSegList_U,
				Ratio_U,pCurveList_U,pEndList_U,pBlendCurve_U,pCon_BZ_S,pWts_BZ_S);
		if(nResult == MA_ERROR)
			return nResult;
		/////////////////////////////////////////////////////////////////////////// V,T-direction
		nResult = LatticeCurves_V(PatchID,pNodeList,n_Nodes_U,n_Nodes_V,ElSegList_V,
				Ratio_V,pCurveList_V,pEndList_V,pBlendCurve_V,pCon_BZ_T,pWts_BZ_T);
		if(nResult == MA_ERROR)
			return nResult;
		////////////////////////////////////////////////////////// EndInfo at Lattice
																// EndPts Along T-dir
		// If NOT CLosed, need to Generate TangentRibbon_V at
		// 2 Ends of CurveList_V: 
		// For this, we need Tangent_V at 2 End Lattice pts &
		// Twist for 4 Corners: U0V0,UNV0,U0VM, & UNVM
		// For Adini Twist,we need Tangents at Lattice Pts.
		//		U0V0,U0V1,U1V0,U1V1;					Corner_U0V0
		//		UNV0,UNV1,U(N-1)V0,U(N-1)V1;			Corner_UNV0
		//		U0VM,U1VM,U0V(M-1),U1V(M-1);			Corner_U0VM
		//		UNVM,UNV(M-1),U(N-1)VM,U(N-1)V(M-1);	Corner_UNVM
		///////////////////////////////////////////////////////////
		// So, for Adini Twist:
		//		for Each Curve in both direction,we create
		//		4 Tangents i.e., 2 for each End
		///////////////////////////////////////////////////////////
		// So, for Spline Twist: NOT IMPLEMENTED YET
		//		for Each Curve in both direction,we NEED
		//		ALL Tangents at Lattice Pts
		/////////////////////////////////////////////////////////////////////////////
		if(!pBlendCurve_V->IsClosed_S())
		{
			nResult = CreateCurveEndTanList(pCurveList_U,EndTanList_U);
			if(nResult == MA_ERROR)
				return nResult;
			///////////////////////////////////////////////////////////
			nResult = CreateCurveEndTanList(pCurveList_V,EndTanList_V);
			if(nResult == MA_ERROR)
				return nResult;
			///////////////////////////////////////////////////////////
		}
		/////////////////////////////////////////////////////////////// EndListExt
		E
	}
	/////////////////////////////////////////////////////////////////////////////// Interior BezCurves 
																				 // in T-dir 
	PostPatBezInfoUsingBlendingInfo(pDrPatch,ElperSegList_V,pEndListExt_V,pBlendCurve,Ratio,
							pCon_BZ_S,pWts_BZ_S,nCon_BZ,nCurves,nCon_BZ_T,nBezSeg_T,nCurDir);
	////////////////////////////////////////////////////////// Post OtherInfo
	m_PatchType		= PA_TENSOR;
	////////////////////////
	nResult = SetInfoForDataBase(pDrPatch,pCurveList,pLatchList,pBlendCurve,nCurDir);
	/////////////////////
	return nResult;
}
//////////////////////////////////////////////////////////////////////////////////// S-Direction
int CMP_Tensor::LatticeCurves 
				(
					int								nDir,
					CString&						PatchID,		
					CDListMgr*						pNodeList,		// Lattice
					int								n_Curves,		// = nNode_V
					//////////////////////////////////////////////
					int								n_Nodes,
					CList<int,int>&					ElperSegList,
					double							Ratio,
					CDListMgr*						pCurveList,	
					CList<BLENDEND,BLENDEND>*		pEndList,		// Given Curves End Conds in U-dir
																		// or BlendCurve End Conds in U-dir
																		// as many as n_Nodes_V
					CDrCurve*						pBlendCurve,
					///////////////////////////////////////////////
					pWORLD							pCon_BZ, 	
					pDOUBLE							pWts_BZ
				) 
{
	int nResult = MA_OK;
	int nCon_BZ,nBezSeg;
	///////////////////////////////////////////////////////////////////////////////// U,S-direction
	nResult = CreateCurveList(PatchID,pNodeList,&CurveList,ElSegList,
							pEndList,pBlendCurve,nNode,n_Curves,nConBZ,nBezSeg)
	/////////////////////////
	int		nDim,nDegree,nMaxOSeg,nOut;
	BOOL	bUnifOT;
	/////////
	int nDim			= pBlendCurve->GetnDim_S(); 
	nDegree				= pBlendCurve->GetOrder_S() - 1; 
	Process_OutputInfo(&ElSegList,nDegree,nBezSeg,Ratio,bUnifOT,nMaxOSeg,nOut);
	////////////////////////////////////////////////////// Info
	if(nDir == 1)	// S-dir
	{
		m_nOrder_S			= nDegree + 1;
		m_bClosed_S			= pBlendCurve->IsClosed_S();
		m_nDim_S			= nDim;
		m_CurveType_S		= pBlendCurve->GetCurveType_S();
		m_nSegs_S			= nBezSeg;				
		m_nMaxPatchCNodes_S	= nCon_BZ;
		m_nOut_S			= nOut;
		m_nMaxOSeg_S		= nMaxOSeg;
		m_bUnifOT_S			= bUnifOT;
		m_pNum_t_S			= m_pOSeg;
		m_pDis_t_S			= m_pODis;
	}
	else
	if(nDir == 2)	// T-dir
	{
		m_nOrder_T			= nDegree + 1;
		m_bClosed_T			= pBlendCurve->IsClosed_S();
		m_nDim_T			= nDim;
		m_CurveType_T		= pBlendCurve->GetCurveType_S();
		m_nSegs_T			= nBezSeg;				
		m_nMaxPatchCNodes_T	= nCon_BZ;
		m_nOut_T			= nOut;
		m_nMaxOSeg_T		= nMaxOSeg;
		m_bUnifOT_T			= bUnifOT;
		m_pNum_t_T			= m_pOSeg;
		m_pDis_t_T			= m_pODis;
	}
	////////////////////////////////////////////////////////// Get Lattice Curves BezPts & Wts
	int nCurves		= CurveList.GetCount(); 
	int nCon_BZTot	= nCon_BZ * nCurves;  
	pWORLD  pCon_BZ = new WORLD [nCon_BZTot]; 	
	pDOUBLE pWts_BZ = new double[nCon_BZTot];
	/////////////////////////////////////////
	int nTopDegree	= nDegree; 
	//////////////////////////////////////////////////////
	//	To save Programming Time use Elevate Routines etc
	//	To get BezPts & Wts Info( Obviously,Elevation will
	//	NOT be done there, because all Curves are Blended
	//	same Order; but,then we need Latchlist filled with
	//	all CL_FORWARD
	//////////////////////////////////////////////////////
	CList<CURVELATCH,CURVELATCH> LatchList;
	for(i=0;i<nCurves;i++)
		LatchList.AddTail(CL_FORWARD);
	nResult = ProcessCurves(&CurveList,&LatchList,pCon_BZ,pWts_BZ,nCon_BZ,
											nCon_BZTot,nTopDegree,nDim);
	///////////////////////
}
//////////////////////////////////////////////////////////////////////////////////// PreProcess
int CMP_Tensor::CreateCurveList(CString& PatchID,CDListMgr* pNodeList,
							CDListMgr* pCurveList,CList<int,int>& ElSegList,
							CList<BLENDEND,BLENDEND>* pEndList,CDrCurve* pBlendCurve,
							int nNode,int n_Curves,int& nConBZ,int& nBezSeg)
{
	///////////////////////////////////////////////////////////
	int i;
	CDListMgr	INodeList;
	POSITION pos;
	BLENDEND BlendEnd;
	//////////////////////////////////////////////////////// BlendInfo
	BOOL bClosed			= pBlendCurve->IsClosed_S();						 
	BOOL nDegree			= pBlendCurve->GetOrder_S() - 1;						 
	//////////////////////////////////////////////////////// Bez
	nBezSeg					= (bClosed)?nNode:nNode-1;
	nConBZ					= (bClosed)?(nDegree*nBezSeg):(nDegree*nBezSeg+1);
	//////////////////////////////////////////////////////// Loop Over Bez. Pts. on Curves
	for(i=0;i<nCurves;i++)
	{
		if(!bClosed)	// otherwise pEndList Empty
		{
			pos			= pEndList->FindIndex(i); 
			BlendEnd	= pEndList->GetAt(pos);
		}
		//////////////////////////////////////////////////// Input Nodes 
		INodeList.RemoveAll();
		PackINodeList(pNodeList,i,nNode,&INodeList);
		//////////////////////////////////////////////////// Now Generate
		CDrCurve* pCurve = CreateCurve(BlendEnd,pBlendCurve,&INodeList,
												ElSegList,PatchID);
		//////////////////////////////////////////////////// Save
		if(pCurve)
			pCurveList->InsertObject(pCurve);
	}
	//////////////
	return MA_OK;
}

int CMP_Tensor::PackINodeList(CDListMgr* pNodeList,iCurve,int nNode,CDListMgr* pINodeList)
{
	int i,nBeg,nEnd;
	POSITION pos;
	CDrNode* pNode;
	////////////////
	nBeg = iCurve * nNode;
	nEnd = nBeg + nNode;
	////////////////////////
	pINodeList->RemoveAll();
	////////////////////////
	for(i=nBeg,i<nEnd;i++)
	{
		pos		= pNodeList->FindIndex(i);
		pNode	= (CDrNode*)pNodeList->GetAt(pos);
		/////
		if(pNode)
			pINodeList->InsertObject(pNode);
		else
			return MA_ERROR;
	}
	/////////////
	return MA_OK;
}

CDrCurve* CMP_Tensor::CreateCurve(BLENDEND BlendEnd,CDrCurve* pBlendCurve,CDListMgr* pINodeList,
														CList<int,int>& ElSegList,CString& PatchID)
{
	// pINodeList contains already evaluated Bez Pts. of the data curves which
	// will now serve as INPUT for Blending Curve Generation whose Bez. Pts.
	// become Patch Bez Pts. for current column, nCol
	///////////////////////////////////////////////////////////////////////////
	CDrGrafFrame* pWnd		= (CDrGrafFrame*)AfxGetMainWnd();
	/////////////////////////////////////////////////////////
	CDrGrafDoc* pDoc 		= ((CDrGrafApp*)AfxGetApp())->GetDocument(); 
	CObjectMgr* pObjectMgr  = pDoc->GetObjectMgr(); 
	CDListMgr*	pList;
	int nActiveIndex;
	CDrCurve* pCurve;
	///////////////////
	pObjectMgr->SetActiveObjectType(CURVE);
	////////////////////////////////////////////////////// Add to DataBase
	CString	ID;
	pObjectMgr->SetObjInfoID(ID,CURVE);
	ID = "_" + PatchID;
	///////////////////////////////////////// delete & Add
	pList = pObjectMgr->GetObjectList(CURVE);
	nActiveIndex = pList->GetObjectIndex(CURVE, ID);
	if(nActiveIndex>=0)
	{
		pObjectMgr->DeleteFromDataBase(nActiveIndex, CURVE);
		pCurve = (CDrCurve*)NULL;
	}
	pCurve	= (CDrCurve*)(pObjectMgr->AddToDataBase(ID,nActiveIndex,CURVE));
	///////////////////////////////////////// Get rid of Old CNodes & INodes
	if(!(pCurve->GetCNodeList()->IsEmpty())  )
		pCurve->GetCNodeList()->RemoveAll();
	if(!(pCurve->GetINodeList()->IsEmpty())  )
		pCurve->GetINodeList()->RemoveAll();
	/////////////////////////////////////////////////////////////// Clone Info
	pCurve->SetObjectInfo(pBlendCurve);
	/////////////////////////////////////////////////////////////// End Conditions
	pCurve->SetSetBCL_S(BlendEnd.m_BCL);
	pCurve->SetSetBCR_S(BlendEnd.m_BCR);
	pCurve->SetTanL_S(BlendEnd.m_TanL);
	pCurve->SetTanR_S(BlendEnd.m_TanR);
	pCurve->SetWtType_S(WT_UNIFORM);	// UNIFORM
	////////////////////////////////////
	pCurve->SetShow(FALSE);	// not quite ready
	/////////////////////////////////////////////////////////////// Fill Input Nodes
	pCurve->GetINodeList()->AddTail(pINodeList);	// copy
	///////////////////////////////////////////////////////
	pCurve->SetRatio_S(Ratio);
	pCurve->SetClosed_S(pBlendCurve->IsClosed_S());
	pCurve->SetCurveType_S(pBlendCurve->GetCurveType_S()); 
	pCurve->SetObjectID(ID);
	/////////////////////////////////////////////////////////////// Do it
	nResult = Do_Curve(pCurve,ElSegList);
	////////////
	if(nResult == MA_ERROR)
		return (CDrCurve*)NULL;
	else
		return pCurve;
}

int CMP_Tensor::Do_Curve(CDrCurve* pCurve,CList<int,int>& ElSegList)
{
	CDListMgr* pINodeList	= pCurve->GetINodeList();
	CString ID				= pCurve->GetObjectID();
	CURVEPROC CurveType		= pCurve->GetCurveType_S(); 
	/////////////////////////////////////////////////////////////// Using GangC
	CMI_GangC	GangC;
	CMC_Circle	MC_Circle;
	CMC_Parab	MC_Parabola;
	CDrNode*	pFromNode = NULL;
	CDrNode*	pToNode = NULL;
	/////////////////
	switch(CurveType)
	{
		case CP_CIRCLE:	// Circle by NO3

			////////////////////////////////////
			pCurve = MC_Circle.CreateCircle(
								ID,pCurve,pINodeList,CP_CIRCLE,
								&ElSegList,CIRCLE_BY_NO3,0.0,120/*SegAngle*/,0.0,
								TRUE/*bArc*/,pFromNode,pToNode,FALSE/*bFinal*/);
			/////////////////
			break;
				
		case CP_PARABOLA:

			////////////////////////////////////
			pCurve = MC_Parabola.CreateParabola(
								ID,pCurve,pINodeList,CP_PARABOLA,
								&ElSegList,/*m_bAdvanceOn*/FALSE,FALSE/*bFinal*/);
			/////////////////
			break;
			
		default:

			pCurve = GangC.CreateCurve(
								ID,pCurve,pINodeList,CurveType,
								&ElSegList,FALSE/*m_bAdvanceOn*/,FALSE/*bFinal*/);
			/////////////////
			if(!pCurve)
				return MA_ERROR;
			//////
			break;
	}
	///////////////////////////////
	return MA_OK;
}

